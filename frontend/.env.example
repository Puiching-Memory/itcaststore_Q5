# Langflow Configuration
# The base URL for Langflow API (e.g., http://langflow:7860/api)
VITE_LANGFLOW_BASE_URL=http://langflow:7860/api

# The unique ID of your Langflow flow
VITE_LANGFLOW_FLOW_ID=your-flow-id-here

# Your Langflow API Key (if required)
VITE_LANGFLOW_API_KEY=your-api-key-here

# Backend API Configuration
# The base URL for the Spring Boot backend API
VITE_API_BASE_URL=http://localhost:8080/api
